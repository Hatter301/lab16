#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i,n,j=0,s,f=0;
	int* a;
	int* b;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		a = (int*)malloc(n * sizeof(int));
		for (i = 0; i < n; i++)
			cin >> a[i];
		for (i = 0; i < n; i++)
			if (a[i] < 0)
				f += 1;
		if (f == 0) {
			for (i = 0; i < n; i++)
				cout << a[i] << " ";
			free(a);
		}
		else {
			s = n + f;
			b = (int*)malloc(s * sizeof(int));
			for (i = f; i < s; i++) {
				b[i] = a[j];
				j += 1;
			}
			for (i = f; i < s; i++)
				if (b[i] < 0) {
					for (j = 0; j < i; j++)
						b[j] = b[j + 1];
					b[i] = 0;
				}
			for (i = 0; i < s; i++)
				cout << b[i] << " ";
			free(a);
			free(b);
		}
	}
	return 0;
}
