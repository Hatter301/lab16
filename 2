#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i, n,j,k,s=1,x,t,y;
	int* a;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		k = n - 1;
		a = (int*)malloc(n * sizeof(int));
		for (i = 0; i < n; i++)
			cin >> a[i];
		for (i = 0; i < k+1; i++) {
			for (j = 0; j < k+1; j++)
				if (a[i] == a[j])
					s += 1;
			if (s == 3) {
				t=a[i];
				for (x = i; x < k; x++)
					a[x] = a[x + 1];
				k -= 1;
				for (x = i + 1; x < k; x++)
					if (a[x] == t)
						for (y = x; y < k; y++)
							a[y] = a[y + 1];
				k -= 1;
			}
			s = 1;
		}
		cout << k+1 << endl;
		for (i = 0; i <= k; i++)
			cout << a[i] << " ";
		free(a);
	}
	return 0;
}
