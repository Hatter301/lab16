#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i, n,min,max,k,j;
	int* a;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		min = INT_MAX;
		max = -1 * INT_MAX;
		k = n + 2;
		a = (int*)malloc(k * sizeof(int));
		for (i = 2; i < k; i++)
			cin >> a[i];
		for (i = 2; i < k; i++) {
			if (a[i] > max)
				max = a[i];
			if (a[i] < min)
				min = a[i];
		}
		for (i=2;i<k;i++)
			if (a[i] == min) {
				for (j = 0; j < i-1; j++)
					a[j] = a[j + 1];
				a[i - 1] = 0;
			}
			else if (a[i] == max) {
				for (j = 0; j < i; j++)
					a[j] = a[j + 1];
				a[i] = 0;
			}
		for (i = 0; i < k; i++)
			cout << a[i] << " ";
		free(a);
	}
	return 0;
}
