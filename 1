#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i, n,j,k;
	int* a;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		k = n-1;
		a = (int*)malloc(n * sizeof(int));
		for (i = 0; i < n; i++)
			cin >> a[i];
		for (i=0;i<k;i++)
			if (a[i] == a[i + 1]) {
				for (j = i; j < k; j++)
					a[j] = a[j + 1];
				k -= 1;
				i -= 1;
			}
		for (i = 0; i <= k; i++)
			cout << a[i] << " ";
		free(a);
	}
	return 0;
}
